{
  "schemaVersion": 1,
  "id": "muffins_questlib",
  "version": "${version}",
  "name": "Muffin's Quest Lib",
  "description": "Adds Quest System to Minecraft",
  "authors": ["FireMuffin303"],
  "contact": {},
  "license": "MIT",
  "icon": "assets/muffins_questlib/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "net.firemuffin303.muffinsquestlib.client.MuffinsQuestLibClient"
    ],
    "main": [
      "net.firemuffin303.muffinsquestlib.MuffinsQuestLib"
    ],
    "fabric-datagen": [
      "net.firemuffin303.muffinsquestlib.datagen.ModDataGen"
    ],
    "modmenu": [
      "net.firemuffin303.muffinsquestlib.integration.ModMenuConfig"
    ]
  },
  "mixins": [
    "muffinsquestlib.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
